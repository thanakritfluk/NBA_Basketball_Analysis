            <p class="vs">VS</p>
        </div>
        <div class="col-4-xs team_input">
            <input class="match_input team_input_two" type="text" placeholder="Team name">
        </div>
    </div>
    <div class="chart-wrapper">
        <canvas id="myChart"></canvas>
    </div>
</div>

<script src="/javascripts/typeahead.js"></script>
<script src="/javascripts/team_autocomplete.js"></script>
<script src="/javascripts/matches.js"></script>
</body>

</html>
